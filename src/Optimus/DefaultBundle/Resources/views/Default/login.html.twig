<form action="{{ path("_security_check") }}" method="post" id="login">
        <div>
            <label for="username"><h5 style="color:#45688F;">Телефон или email</label> <br/>
            <input type="text" id="username" size="14em" name="_username" value="{{ last_username }}" />
        </div>

        <div>
            <label for="password" ><h5 style="color:#45688F;">Пароль</label> <br/>
            <input type="password" id="password" name="_password" size="14em"/>
        </div>

        <a href="javascript:void(0);"  class="bt-login">Войти</a> <br/><br/>
        <a href="#" class="bt-login">Регистрация</a>
</form>